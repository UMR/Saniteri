<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.umr.saniteri.ui" android:versionCode="1"
	android:versionName="1.0">
	<supports-screens android:smallScreens="true"
		android:resizeable="true" android:normalScreens="true"
		android:largeScreens="true" android:anyDensity="true" />
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:name=".LogIn" android:label="@string/app_name"  >
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".DeviceList" android:label="@string/app_name" >
			<intent-filter>
				<action android:name="com.umr.saniteri.ui.DeviceList" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".DeviceDetail" android:label="@string/app_name"  >
			<intent-filter>
				<action android:name="com.umr.saniteri.ui.DeviceDetail" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".WebServicePreference" android:label="@string/app_name">
			<intent-filter>
				<action android:name="com.umr.saniteri.ui.WebServicePreference" />
				<category android:name="android.intent.category.PREFERENCE" />
			</intent-filter>
		</activity>
	</application>
	<uses-sdk android:minSdkVersion="8" />
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
	

</manifest> 